          }
        case 89: break;
        case 11: 
          { return new Symbol(sym.Multiplicacion, yychar, yyline, yytext());
          }
        case 90: break;
        case 40: 
          { return new Symbol(sym.G_label, yychar, yyline, yytext());
          }
        case 91: break;
        case 8: 
          { return new Symbol(sym.Suma, yychar, yyline, yytext());
          }
        case 92: break;
        case 27: 
          { return new Symbol(sym.A_mod, yychar, yyline, yytext());
          }
        case 93: break;
        case 46: 
          { return new Symbol(sym.E_mediana, yychar, yyline, yytext());
          }
        case 94: break;
        case 51: 
          { return new Symbol(sym.G_graphpie, yychar, yyline, yytext());
          }
        case 95: break;
        case 54: 
          { return new Symbol(sym.G_graphline, yychar, yyline, yytext());
          }
        case 96: break;
        case 36: 
          { return new Symbol(sym.G_exec, yychar, yyline, yytext());
          }
        case 97: break;
        case 20: 
          { return new Symbol(sym.Punto, yychar, yyline, yytext());
          }
        case 98: break;
        case 1: 
          { System.err.println("Carácter no reconocido: " + yytext());
    return new Symbol(sym.ERROR, yychar, yyline, yytext());
          }
        case 99: break;
        case 30: 
          { return new Symbol(sym.A_div, yychar, yyline, yytext());
          }
        case 100: break;
        case 4: 
          { /*Ignore*/
          }
        case 101: break;
        case 22: 
          { return new Symbol(sym.C_dobles_c, yychar, yyline, yytext());
          }
        case 102: break;
        case 23: 
          { return new Symbol(sym.Doble_puntos, yychar, yyline, yytext());
          }
        case 103: break;
        case 41: 
          { return new Symbol(sym.Column, yychar, yyline, yytext());
          }
        case 104: break;
        case 7: 
          { return new Symbol(sym.Igual, yychar, yyline, yytext());
          }
        case 105: break;
        case 9: 
          { return new Symbol(sym.Coma, yychar, yyline, yytext());
          }
        case 106: break;
        case 2: 
          { return new Symbol(sym.Identificador, yychar, yyline, yytext());
          }
        case 107: break;
